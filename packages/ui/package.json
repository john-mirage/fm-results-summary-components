{
  "name": "ui",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "tsup src/main.js --format esm,cjs --watch",
